{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "swacosmosdb"
    },
    "location": {
      "value": "Central US"
    },
    "sku": {
      "value": "Free"
    },
    "skucode": {
      "value": "Free"
    },
    "repositoryUrl": {
      "value": "https://github.com/jeffwmartinez/swa-example-2.git"
    },
    "branch": {
      "value": "main"
    },
    "repositoryToken": {
      "value": "<GITHUB-TOKEN>"
    },
    "Provider": {
      "value": "GitHub"
    },
    "appLocation": {
      "value": "/src/web"
    },
    "apiLocation": {
      "value": "/src/api"
    },
    "appArtifactLocation": {
      "value": "build"
    },
    "stagingEnvironmentPolicy": {
      "value": "Enabled"
    },
    "allowConfigFileUpdates": {
      "value": true
    },
    "resourceTags": {
      "value": {
        "Environment": "Development",
        "Project": "Testing SWA with ARM",
        "ApplicationName": "swacosmosdb"
      }
    },
    "appSettings": {
      "value": {
        "COSMOS_CONNECTION_STRING": "<PRIMARY-KEY>"
      }
    }
  }
}